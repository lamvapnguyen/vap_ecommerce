<setting name="SEO">
<![CDATA[

<input type="hidden" value="1" name="update_hidden_field" />
<table cellspacing="0" cellpadding="0" width="100%" class="dm_edit_table">
	<col width="200px" /><col width="1px" /><col />
	<tr><td>&nbsp;</td><td></td><td></td></tr>
	<tr>
		<td class="dm_ioc_value_t">Google Site Verification</td>
		<td class="dm_ioc_value_s">:</td>
		<td class="dm_ioc_value">
			<input type="text" name="google_site_verification" value="{p:google_site_verification}" class="ioc_textbox" style="width:90%;color:#090;" /><br/>
			<input type="text" name="google_site_verification_2" value="{p:google_site_verification_2}" class="ioc_textbox" style="width:90%;color:#090;" />
		</td>
	</tr>
	<tr>
		<td class="dm_ioc_value_t" style="vertical-align:top">Google Analytics</td>
		<td class="dm_ioc_value_s" style="vertical-align:top">:</td>
		<td class="dm_ioc_value">
			<textarea type="text" class="ioc_textbox" maxlength="4000" rows="9" name="gganalytics" style="height:auto;" />{p:gganalytics}</textarea>
		</td>
	</tr>
	<tr><td>&nbsp;</td><td></td><td></td></tr>
	<tr>
		<td class="dm_ioc_value_t">{lang:meta_refresh_time}</td>
		<td class="dm_ioc_value_s">:</td>
