				<input type="checkbox" value="True" name="seo_nav_h2" {p:seo_nav_h2} />&nbsp;<span class="span_label">{lang:seo_nav_h2}</span>
			</div>
			<div>
				<input type="checkbox" value="True" name="seo_title_link" {p:seo_title_link} />&nbsp;<span class="span_label">{lang:seo_title_link}</span>
			</div>
			<div>
				<input type="checkbox" value="True" name="seo_title_vn" {p:seo_title_vn} />&nbsp;<span class="span_label">{lang:seo_title_vn}</span>
			</div>
			<div>
				<input type="checkbox" value="True" name="on_rich_snippets" {p:on_rich_snippets} />&nbsp;<span class="span_label">Rich Snippets</span>
			</div>
